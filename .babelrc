{
  "presets": [
    [
      "env", {
        "targets": {
          "node": "current"
        },
        "test": {
          "plugins": ["istanbul"]
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@": "./src"
      }
    }],
    ["extensible-destructuring"]
  ]
}
