{
  "domains": {
    "remote": "shokku.com",
    "local": "shokku.lan"
  },
  "email": "example@domain.com",
  "docker": {
    "registry": {
      "remote": "registry.shokku.com",
      "local": "shokku"
    }
  },
  "s3": {
    "bucket": "mys3.shokku.com"
  },
  "projects": {
    "api": {
      "enabled": true,
      "subdomains": ["api"],
      "networks": ["ubiq"],
      "docker": {
        "images": [{
            "name": "api",
            "folder": "api",
            "dockerfile": "Dockerfile",
            "version": "1.0.0",
            "build": "."
          },
          {
            "name": "api-dev",
            "folder": "api",
            "dockerfile": "Dockerfile.dev",
            "version": "1.0.0",
            "build": ".",
            "dev": true
          },
          {
            "name": "traefik",
            "folder": "traefik",
            "dockerfile": "Dockerfile",
            "version": "1.0.0",
            "build": "docker/images/traefik"
          },
          {
            "name": "gubiq",
            "folder": "gubiq",
            "dockerfile": "Dockerfile",
            "version": "1.0.0",
            "build": "docker/images/gubiq"
          }
        ]
      }
    },
    "indexer": {
      "enabled": true,
      "docker": {
        "images": []
      }
    }
  }
}
